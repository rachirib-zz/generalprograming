	<mapping resource="co\com\fc\bd\model\FacrPTipoParametro.hbm.xml" />
	<mapping resource="co\com\fc\bd\model\FacrReferencias.hbm.xml" />
	<mapping resource="co\com\fc\bd\model\FacrSolicitante.hbm.xml" />
	<mapping resource="co\com\fc\bd\model\FacrUsuario.hbm.xml" />
